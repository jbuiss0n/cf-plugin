{
    "name": "cf-templates",
    "displayName": "Cloud Formation Templates",
    "description": "Provide support for code completion and validation of AWS Cloud Formation templates",
    "version": "0.0.1",
    "publisher": "pradiphudekar",
    "repository":"https://github.com/krishnan-mani/cf-plugin",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [],
        "jsonValidation": [
            {
                "fileMatch": "*.cf.json",
                "url": "./schema/schema.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}